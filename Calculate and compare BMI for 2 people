const weightMark = 63;
const heightMark = 1.69;
const weightJohn = 92;
const heightJohn = 1.95;

const bmiMark = weightMark / (heightMark ** 2)
const bmiJohn = weightJohn / (heightJohn ** 2)
console.log(bmiMark);
console.log(bmiJohn);

if (bmiMark > bmiJohn) {
    console.log(`Mark's BMI is higher. Mark has BMI of ${bmiMark} while John has ${bmiJohn}`)
} else {
    console.log(`John's BMI is higher. Mark has BMI of ${bmiMark} while John has ${bmiJohn}`)
};
